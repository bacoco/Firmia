# INSEE API Configuration
# Option 1: Legacy API (will be deprecated in September 2024)
INSEE_API_KEY=your_legacy_insee_api_key

# Option 2: New API with Integration Key (Recommended)
INSEE_API_KEY_INTEGRATION=c1e98007-96f9-498a-a980-0796f9a98a23

# Option 3: New API with OAuth2 Client Credentials (Most Secure)
INSEE_CLIENT_ID=your_client_id
INSEE_CLIENT_SECRET=your_client_secret

# Banque de France API Configuration
BANQUE_FRANCE_API_KEY=your_banque_france_api_key

# INPI API Configuration
INPI_USERNAME=your_inpi_username
INPI_PASSWORD=your_inpi_password

# Cache Configuration
CACHE_TTL=3600
CACHE_CHECK_PERIOD=600

# Rate Limiting Configuration
RATE_LIMIT_INSEE=5000
RATE_LIMIT_BANQUE_FRANCE=1000
RATE_LIMIT_INPI=2000